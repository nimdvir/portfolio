function ShowMaavaron(appl, categoryId, numBan )
{
   var maavaron_obj          = GetMaavaron(appl, categoryId);
   var banner_path           = maavaron_obj[numBan]['banner_path'];
   var baner_html            = (banner_path.indexOf('.html')>0?1:0);

   document.write('<span style="display:table-cell;vertical-align:middle;height:98%;width:99%;border:0px solid red;position:absolute;text-align:center" id="Maavaron_div">');
   document.write('<div style="display:block;float:left;height:50%;margin-top:-233px;visibility:hidden;width:100%;">&nbsp;</div>');
   document.write('<table align="center" dir="rtl" border="0" width="960" style="margin:0 auto;">');
   document.write('<tr><td align="right" valign="bottom" width="908"><a href="https://www.makorrishon.co.il/nrg"><img height="38" width="218" alt="מעריבnrg" src="/nrg/images/nrg/include/img/header/nrg_logo.gif" border="0"/></a></td>');
   document.write('<td valign="bottom" width="52" class="skip"><span id="skip_btn" onclick="CloseMaavaron();return false;" style="cursor:pointer"><img alt="דלג" title="דלג" src="/nrg/app/staticfiles/mskip2.gif" border="0"/></span></td></tr>');
   document.write('<tr><td align="right" height="7" style="background:#c00000;" colspan="2"></td></tr><tr><td align="right" height="2" colspan="2"></td></tr><tr>');
   document.write('<tr><td align="center" colspan="2">');


  if (typeof maavaron_obj[numBan]['show_time'] != 'undefined') var show_time = maavaron_obj[numBan]['show_time'];
  else var show_time = 10;

  if (baner_html==1){ document.write('<iframe id="bannerFrm" src="'+ banner_path + '" allowTransparency="true" width="960" height="400" marginwidth="0" marginheight="0"  hspace="0" vspace="0" scrolling="no" frameborder="0"></iframe>');
  } else if (navigator.appName == 'Microsoft Internet Explorer'){ document.write('<OBJECT classid=clsid:D27CDB6E-AE6D-11cf-96B8-444553540000 codebase=http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=7,0,0,0  WIDTH="960" HEIGHT="400"><PARAM NAME=movie VALUE="'+banner_path+'" ><PARAM NAME=quality VALUE=high><param name=wmode value=Opaque></OBJECT>');
  } else{ document.write('<embed wmode=Opaque wmode=opaque  name=flashfile10 quality=high type=application/x-shockwave-flash src="'+banner_path+'" width="960" height="400"><\/embed>');}

