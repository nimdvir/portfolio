function CloseMaavaron()
{
if(flagMaavaron) return;
   if (nrg.$('Maavaron_div'))
   {  document.getElementById('Maavaron_div').parentNode.removeChild(document.getElementById('Maavaron_div'));
      if (nrg.$('footer'))
      {  recoverBanners();
                           if (nrg.$('header')) addTabsEvent();
         if (nrg.$('page-wrap')&& (nrg.$('page-wrap').className == 'articlePage')) GetColumnHeightDifference();
      } else
      {  nrg.onBodyLoad(recoverBanners);
         if (nrg.$('header')) nrg.onBodyLoad(recoverBanners);
                                    if (nrg.$('page-wrap')&& (nrg.$('page-wrap').className == 'articlePage')) nrg.onBodyLoad(GetColumnHeightDifference);
      }
      flagMaavaron = true;
   }
}

function recoverBanners(){
    var iframes = window.frames;
    for (var i=0;i<iframes.length;i++){
        try {
           if((typeof(iframes[i].window.setHeight)!= "undefined")){
                iframes[i].window.setHeight();
             } else if(typeof(iframes[i].window.setIframe)!= "undefined"){
              iframes[i].window.setIframe();

             }
                         else if(typeof(iframes[i].window.showit)!= "undefined"){
                            iframes[i].window.showit();
                         }

        } catch(e){}
    }
}

// OTHER FUNCTIONS

// =================
function GetMaavaron ( myAppl, myCat)
{ var maavaron_obj=false;
  var maavaron_ctr = nrg.getCookie("locusr") != "" ? nrg.getCookie("locusr") : "N" ;
  if (typeof  maavaron_conf[myAppl][myCat]!= 'undefined')  maavaron_obj = maavaron_conf[myAppl][myCat][maavaron_ctr];
  else  maavaron_obj = maavaron_conf[myAppl][maavaron_ctr];
  return maavaron_obj;
}

// === Set Maavaron Cookies ===
function maavaron_SetCookie(name, value) {
  var days=1;
  var expire = new Date ();
  expire.setTime (expire.getTime() + (24 * 60 * 60 * 1000) * days);
  document.cookie = name + "=" + escape(value) + "; expires=" + expire.toGMTString()+'; path=/ ';
}




