var videoPromo = {
	targetEl:[],
	mainElementId: "videoPromo", 
	init: function(){
		
		if (nrg.$(this.mainElementId)){
			var elements = document.getElementsByTagName('DIV');
			this.addEvent(nrg.$(this.mainElementId),'mouseout',function(){
				videoPromo.removeAllHovers();
			});
			for(var i=0;i< elements.length;i++){
				if (elements[i].className == 'mainVideo' || elements[i].parentNode.className == 'videoItems'){
						this.makeHover(elements[i]);
						this.addEvent(elements[i],"mouseover",function(){
							videoPromo.toggleHover(this);
						});						
						this.targetEl.push(elements[i]);
				}
			} 		
		}	
	},	
makeHover: function(el){
			var div = document.createElement('DIV');
			var url = this.getRedirectUrl(el);
			div.className = 'vidOver';			
			this.addEvent(div,'click',function(){
				window.location.href = url.url;  			
			});
			
			div.innerHTML = '<span class="playIcn"></span><div class="overlayBg"></div>';
			
			this.addEvent(div,"mouseout",function(){
						videoPromo.removeHover(this);
			});
			this.addEvent(div,"click",function(){
					window.location.href = url.url;
			});
			el.appendChild(div);
			el.style.zoom = '1';
			if (/msie|MSIE 6/.test(navigator.userAgent)){
					div.style.display = "block";
					div.style.visibility = "hidden";
			}
	},
	getRedirectUrl : function(el){
		var ele = el.getElementsByTagName('H4')[0];
		var a = ele.getElementsByTagName('A')[0];
	 return {
			txt: a.innerHTML,
			url: a.href
		}
	},
	removeAllHovers: function(){
		for (var i=0;i< this.targetEl.length;i++){
			var overEl = this.targetEl[i].getElementsByTagName('DIV')[0];
			if (overEl.style.display == 'block'){
				overEl.style.display = 'none';
			}
		}	
	},
	toggleHover: function(el){
			var div = el.getElementsByTagName('DIV')[0];
			div.style.display = 'block';
			if (/msie|MSIE 6/.test(navigator.userAgent)){
					div.style.visibility = "visible";
			}

		/*	this.removeEvent(el,"mouseover",function(){
							videoPromo.toggleHover(this);
			});
		*/
	},
	removeHover: function(el){
			el.style.display = 'none';	
	},	
	addEvent: function (obj, type, fn) {
		if (obj.attachEvent) {
			obj['e' + type + fn] = fn;
			obj[type + fn] = function(){
				obj['e' + type + fn](window.event);
		}
			obj.attachEvent('on' + type, obj[type + fn]);
		}
		else {
			obj.addEventListener(type, fn, false);
		}
},
