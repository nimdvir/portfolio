/* Tabs */
function changeTab(obj,tabStr,divStr,total) {
 	if(document.getElementById(obj).style.backgroundImage=="url(include/img/tab_on.png)") {
		disableSelection(document.getElementById(obj));
		return;
	}
   for(i=1;i<=total;i++) {
   		if(document.getElementById(obj)==document.getElementById(tabStr+i)) {
			//document.getElementById(obj).style.backgroundImage="url(include/img/tab_on.png)";
			//document.getElementById(obj).style.color="#fff";
			document.getElementById(obj).setAttribute("class","mostReadTopTab1");
			document.getElementById(obj).setAttribute("className","mostReadTopTab1");
			document.getElementById(divStr+i).style.display="block";
			disableSelection(document.getElementById(obj));
			document.getElementById(obj).style.cursor = "pointer";
		}
		else {
			//document.getElementById(tabStr+i).style.backgroundImage="url(include/img/tab_off.png)";
			//document.getElementById(tabStr+i).style.color="#A6A6B4";
			document.getElementById(tabStr+i).setAttribute("class","mostReadTopTab2");
			document.getElementById(tabStr+i).setAttribute("className","mostReadTopTab2");
			document.getElementById(divStr+i).style.display="none";
			disableSelection(document.getElementById(obj));
			document.getElementById(tabStr+i).style.cursor = "pointer";
			
		}
   }
}

