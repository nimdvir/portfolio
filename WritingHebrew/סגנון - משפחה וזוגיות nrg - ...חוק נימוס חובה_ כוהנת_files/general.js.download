var gabayMsg = {

	hasLoaded: false,
	url: '/online/11/MassegeGabi.html',
						
	loadJQuery: function()
	{
		if (typeof jQuery == 'undefined')
		{
			if(!gabayMsg.hasLoaded)
			{
				nrg.loadJS('/online/nrg/include/js/2011/jquery-1.5.min.js');
				gabayMsg.hasLoaded = true;
			}
			setTimeout(function(){ gabayMsg.loadJQuery(); },100);
			return;
		} else
		{
			if(!jQuery.isReady)
			{
				jQuery(document).ready(function(){
					gabayMsg.init();
				});
			}
		}
	},
	
	init: function()
	{
		
		jQuery.get(this.url, gabayMsg.onLoadedHandler );
		
	},
	
	registerEvents: function()
	{
		jQuery('#gabayMassege .arrows a').bind('click', gabayMsg.onClickHandler );
	},
	
	onLoadedHandler: function(data)
	{
		var data = jQuery(data);
		jQuery('#gabayMassege .items').append( data ).hide();
		jQuery('#gabayMassege .items div').hide();
		jQuery('#gabayMassege .items div:first-child').show();
		jQuery('#gabayMassege .items').show();
		
		
		gabayMsg.registerEvents();
		
	},
	
	onClickHandler: function(e)
	{
		e.preventDefault(); 
		
		var cls = jQuery(e.currentTarget).attr('class');
		
		var current = jQuery('#gabayMassege .items div:visible');
		var index = current.index();
		var last  = jQuery('#gabayMassege .items div:last-child').index();
		
		switch(cls)
		{
			case 'fr':
				if(index == 0) return;
				else {
					index -=1;
				}
							
			break;
			case 'fl':
				if(index == last) return;
				else {
					index +=1;
				}
			break;
		}
		current.hide();
		jQuery( jQuery('#gabayMassege .items div').get(index) ).show();
		
	}

}

