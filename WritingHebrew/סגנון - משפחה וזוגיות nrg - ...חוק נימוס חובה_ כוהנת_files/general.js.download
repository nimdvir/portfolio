   var objXMLHttp=null;
    try { // Firefox, Opera 8.0+, Safari    
    
		objXMLHttp=new XMLHttpRequest(); 
	}
    catch (e){    
    	
		try{ // Internet Explorer 
		
		   objXMLHttp=new ActiveXObject("Msxml2.XMLHTTP");   
		}
        catch (e){
		   try{
		      objXMLHttp=new ActiveXObject("Microsoft.XMLHTTP"); 
		   }
           catch (e){
		      return objXMLHttp; 
		   }
		}
	}
    return objXMLHttp;
}  

