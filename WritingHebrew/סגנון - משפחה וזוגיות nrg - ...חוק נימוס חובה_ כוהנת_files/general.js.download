					__div.className = 'vidOver';
					__div.className += ' hide';
					__div.innerHTML = '<span class="playIcn"></span><div class="overlayBg"></div>';
					return __div;
			},
			getElement: function(__element){
			 while (__element.nodeName.toLowerCase() != 'li') {
						__element = __element.parentNode;
				}
				return __element;
			},
			over: function(__element){				
				if (__element.nodeName.toLowerCase() != 'li'){
						__element = this.getElement(__element);				
				}				
				var __hover =__element.getElementsByTagName('div');
				for(var i=0;i<__hover.length;i++){
					if (__hover[i].className.indexOf('vidOver') > -1){						 
							__hover[i].className = __hover[i].className.replace(' hide','');
							if (this._lastElement == '')	
								this._lastElement = __hover[i];
							else if(this._lastElement.parentNode.innerHTML != __hover[i].parentNode.innerHTML){
								this._lastElement = __hover[i];
							}else{return null;}								
					}	
				}
		},
		remove: function(__element){
			this._lastElement.className += ' hide';
		}
}
