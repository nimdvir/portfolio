/* functions by yaron */
 /* reusable functions*/
 
  /* shortcut to getElementById (like $ function.  we dont use "$" here so that	*/
  /* if we want to use prototype, jquery, etc. in future, we don't get clashes)	*/
	function get(el) {
	 return document.getElementById(el);
	}
	   
  /* show-hide anything anywhere anytime 											*/
  /* usage sh(el) = toggle; sh(el,1) = block; 2 = inline; anything_else = hide 	*/
	function sh(el,stype) {
	 if(!get(el)) return;
	 var ost=get(el).style;
	 if(!stype) { // toggle
	  myobj=get(el); 
	  if(window.getComputedStyle) var ocs=window.getComputedStyle(myobj,null).display; else var ocs=myobj.currentStyle.display;
	  if(ocs=='none') {
		  switch(stype) {
			  case 1: ost.display='block'; break;
			  case 2: ost.display='inline'; break;
			  default: ost.display='block';
		  }
	  	  return; //quit
	  }
	 }
	 switch(stype) {
		case 1: ost.display='block'; break;
		case 2: ost.display='inline'; break;
		default: ost.display='none';
	 }
	}
   /* end show-hide anything */

 /* pop-up window*/
 // to center in either drection: use "c" for left and/ or top;
  function popwin(url,name,w,h,left,top) {
    if(left=="c") left=(screen.width-w)/2;
	if(top=="c") top=(screen.height-h)/2;
	newWin = window.open(url,name,"width="+w+",height="+h+",resizable=no,scrollbars=no,status=no,left="+left+",top="+top);
//	if(!left && !top) newWin.moveTo((screen.width-w)/2,(screen.height-h)/2); // center
	newWin.focus();
}

