	//in use for 2011 tabs
	
	function doTabs(element){
			
			if (typeof element != 'undefined' ){
				var tabs = element.getElementsByTagName('ul');
				
				for (var i=0;i<tabs.length;i++){
					if (tabs[i].className.indexOf('tabs') > -1){
						break;	
					}	
				}
				var tabs_li = tabs[i].getElementsByTagName('li');
				var divs = element.getElementsByTagName('div');
				var items =[];
				for (var i=0;i<divs.length;i++){
					if (divs[i].parentNode.className.indexOf('content') > -1){
						items.push(divs[i]);
						
					}	
				}
				//tabs fix
				if(navigator.appName == 'Microsoft Internet Explorer'){
				tabs_li[0].style.zoom = 1;
				var ulWidth = (tabs_li[0].offsetWidth+2)* tabs_li.length;					
					tabs[0].style.width = ulWidth+'px';
					tabs[0].style.zoom = 1;
					
				}
				
				
				
				for (var j=0;j<tabs_li.length;j++){
					tabs_li[j].className += ' li'+ (j); 
						var a = tabs_li[j].getElementsByTagName('a')[0];						
						nrg.addEvent(a,'click',function(e){ e = e|| window.event; if (e.preventDefault)e.preventDefault(); else e.returnValue = false;},false);
						nrg.addEvent(tabs_li[j],'click',function(e){e = e|| window.event;
																																															     var target = e.target || e.srcElement;
																																																				target = target.nodeName == 'A' ? target.parentNode.parentNode : target;
																																																				changeTab(target,items,tabs_li);
																																																				return false;});
				} 
			}
	}
	

	
	function changeTab(element,items,tabs){
			if (element.className.indexOf('li') > -1){
				var from = element.className.indexOf('li') + 2;
				var index = parseInt(element.className.substring(from, from+1));
				//alert(index);
				for (var i=0;i<items.length;i++){
					if (i != index ){	
							if (items[i].className.indexOf('hide') == -1)
								items[i].className += ' hide';
							if (tabs[i].className.indexOf('active')> -1)
								tabs[i].className = tabs[i].className.replace('active','');
					}	
				}
				element.className += ' active';
				items[index].className = items[index].className.replace('hide','');
				
			}
	}
	
